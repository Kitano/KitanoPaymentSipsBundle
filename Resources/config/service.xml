<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="kitano_payment_sips.payment_system.class">Kitano\PaymentSipsBundle\PaymentSystem\SipsPaymentSystem</parameter>
    </parameters>

    <services>
        <service id="kitano_payment_sips.payment_system" class="%kitano_payment_sips.payment_system.class%">
            <argument type="service" id="kitano_payment.repository.transaction" />
            <argument type="service" id="logger"/>
            <argument>%kitano_payment_sips.config%</argument>
            <argument>%kitano_payment_sips.bin%</argument>
            <argument>%kitano_payment.config.notification_url%</argument>
            <argument>%kitano_payment.config.internal_back_to_shop_url%</argument>
            <argument>%kitano_payment.config.external_back_to_shop_url%</argument>
        </service>
    </services>

</container>
